{
  "name": "with-reason-parcel",
  "version": "0.1.0",
  "scripts": {
    "dev": "concurrently \"bsb -clean-world -make-world -w\" \"parcel src/index.html --no-cache\"",
    "prebuild": "node tools/clean.js",
    "build": "bsb -make-world | parcel build src/index.html -d dist/client --public-url /",
    "postbuild": "node tools/move.js",
    "start": "node server.js",
    "workbox": "workbox inject:manifest",
    "move:index": "node tools/move.js",
    "clean:dist": "node tools/clean.js",
    "clean": "bsb -clean-world",
    "reset": "bsb -clean-world && rm -r node_modules yarn.lock dist .cache && yarn",
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack": "webpack -w"
  },
  "keywords": [
    "BuckleScript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "helmet": "^3.9.0",
    "morgan": "^1.9.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-ga": "^2.4.0",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.2.2",
    "reason-react": ">=0.3.0",
    "universal-router": "^5.0.0"
  },
  "devDependencies": {
    "bs-chalk": "^0.1.1",
    "bs-express": "^0.0.6",
    "bs-platform": "^2.1.0",
    "concurrently": "^3.5.1",
    "fs-extra": "^5.0.0",
    "parcel-bundler": "^1.4.1",
    "workbox-cli": "^2.1.2",
    "workbox-sw": "^2.1.2"
  }
}